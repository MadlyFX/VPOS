{
  "name": "VPOS",
  "productName": "VPOS",
  "version": "1.0.0",
  "description": "Virtual Production Operating System",
  "main": "src/vposlaunch.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "postinstall": "electron-rebuild",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "Andy Jarosz <ajarosz@loledvirtual.com>",
  "license": "CC BY-NC-SA 4.0",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "VPOS"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "node-hid": "^2.1.1",
    "node-ipc": "^11.1.0",
    "node-osc": "^8.0.3",
    "node-tcp-proxy": "^0.0.27",
    "osc-js": "^2.3.2",
    "serialport": "^10.4.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^2.3.0",
    "udp-proxy": "^1.2.0",
    "usb-detection": "^4.10.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^12.0.1",
    "electron-rebuild": "^2.3.5",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadlyFX/Indiemark-USB-Driver.git"
  },
  "bugs": {
    "url": "https://github.com/MadlyFX/Indiemark-USB-Driver/issues"
  },
  "homepage": "https://github.com/MadlyFX/Indiemark-USB-Driver#readme"
}
